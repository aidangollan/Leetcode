#link to problem https://leetcode.com/problems/remove-duplicates-from-sorted-array/description/
class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        if len(nums) == 0
            return 0
        k = len(set(nums))
        nums_unique = sorted(list(set(nums)))
        for i in range(k):
            nums[i] = nums_unique[i]
        return k
